{
  "name": "telegram-youtube-tiktok-bot",
  "version": "1.0.0",
  "description": "Bot Telegram untuk download MP3 dari YouTube dan video dari TikTok",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "dev": "nodemon bot.js",
    "check": "node check-dependencies.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop telegram-youtube-tiktok-bot",
    "pm2:restart": "pm2 restart telegram-youtube-tiktok-bot",
    "pm2:logs": "pm2 logs telegram-youtube-tiktok-bot",
    "docker:build": "docker build -t telegram-youtube-tiktok-bot .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "telegraf": "^4.15.6",
    "ytdl-core": "^4.11.5",
    "youtube-search-api": "^1.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "axios": "^1.6.2",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "node-id3": "^0.2.6",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "telegram",
    "bot",
    "youtube",
    "tiktok",
    "download",
    "mp3",
    "mp4"
  ],
  "author": "Your Name",
  "license": "MIT"
}
